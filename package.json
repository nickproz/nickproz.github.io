  ],
  "scripts": {
    "start": "gulp",
    "build": "gulp sass && gulp js",
    "git-add": "git add .",
    "git-commit": "npm run git-add; git commit -m 'commit from gulp plugin'",
    "git-push": "npm run git-commit; git push",
    "git-deploy": "npm run git-push; git subtree push --prefix dist origin gh-pages",
    "git-publish": "npm run git-deploy",
    "git-delete-pages": "git push origin :gh-pages"
  },
  "dependencies": {},
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.0",
    "gulp-sass": "^2.3.2",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "~3.0.8",
    "gulp-webserver": "^0.9.1",
    "gulp-exec": "~2.1.3",
    "gulp-clean-css": "^3.0.4"
  }
}
